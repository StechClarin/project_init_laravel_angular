{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "barryvdh/laravel-dompdf": "^1.0",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.1",
        "laravel/framework": "^8.11.2",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.1",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.0",
        "phpoffice/phpspreadsheet": "^1.18",
        "predis/predis": "^1.1",
        "rebing/graphql-laravel": "^5.1",
        "rennokki/laravel-eloquent-query-cache": "3.2.0",
        "sentry/sentry-laravel": "^2.8",
        "spatie/laravel-permission": "^3.13",
        "stevebauman/location": "^6.1"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/RefactoringItems/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "config-server-base": [
            "cp GraphQLController_local.php vendor/rebing/graphql-laravel/src/GraphQLController.php",
            "sudo chmod -R 777 storage/ bootstrap/cache",
            "rm -rf bootstrap/cache/* storage/framework/views/*",
            "php artisan clear-compiled",
            "composer dumpautoload",
            "php artisan config:clear",
            "php artisan config:cache",
            "mkdir -p public/uploads/users  public/uploads/fournisseurs  public/uploads/clients  public/uploads/ordretransits public/uploads/typeimportations public/uploads/typedossiers public/uploads/entrepots, public/uploads/dossiers",
            "sudo chmod -R 777 public/uploads/ storage/ bootstrap/cache",
            "mkdir -p public/uploads/icons",
            "cp -R public/assets/media/svg/icons/sidebar/* public/uploads/icons/"
        ],
        "config-server-guindy_manager": [
            "@config-server-base",
            "chown -R www-data:www-data storage bootstrap/cache"
        ],
        "config-server-mac": [
            "@config-server-base",
            "sudo chown -R _www:_www storage bootstrap/cache"
        ],
        "config-server-linux": [
            "cp GraphQLController_local.php vendor/rebing/graphql-laravel/src/GraphQLController.php",
            "@config-server-guindy_manager",
            "sudo chmod -R 777 public/uploads/"
        ],
        "config-server-windows": [
            "cp GraphQLController_local.php vendor/rebing/graphql-laravel/src/GraphQLController.php",
            "@config-server-base"
        ],
        "update-server-base": [
            "git fetch --all",
            "git reset --hard origin/develop",
            "git pull",
            "@config-server-guindy_manager"
        ],
        "update-server-guindy_manager": [
            "@update-server-base",
            "cp .env.server .env",
            "php artisan config:cache",
            "gulp prod",
            "cp laravel-echo-server_server.json laravel-echo-server.json"
        ],
        "update-server-guindy_manager-test": [
            "@update-server-base",
            "cp .env.server-test .env",
            "php artisan config:cache",
            "gulp test",
            "cp laravel-echo-server_server.json laravel-echo-server.json"
        ]
    }
}
