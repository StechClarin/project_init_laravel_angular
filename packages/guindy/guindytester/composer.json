{
  "name": "guindy/guindytester",
  "description": "Package Laravel pour l'automatisation des tests REST & GraphQL avec explication d'erreurs via ChatGPT",
  "version": "0.1.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stech Clarin",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/process": "^5.0 || ^6.0",
    "openai-php/client": "^0.5.0"
  },
  "autoload": {
    "psr-4": {
      "Guindy\\GuindyTester\\": "src/"
    },
    "classmap": [
      "src/config"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guindy\\GuindyTester\\GuindyTesterServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "php artisan guindytester:run"
  },
  "keywords": [
    "laravel",
    "test",
    "graphql",
    "rest",
    "chatgpt",
    "ci",
    "guindy"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
